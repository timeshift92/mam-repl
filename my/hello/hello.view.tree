$my_hello $mol_book
	Placeholder $mol_book_placeholder
		minimal_width 600
		title <= placeholder \Stack Overflow
		sub / 
			<= Info $mol_deck
				items /
					<= treeSource *
						title <= treeLabel \.tree
						Content <= TreeSource $mol_textarea
							value?val <=> tree_source?val \
							hint <= tree_source_hint @ \view.tree source
					<= tsSource *
						title <= tsLabel @ \.ts
						Content <= TsSource $mol_textarea
							value?val <=> ts_source?val \
							hint <= ts_source_hint @ \view.ts source
					<= cssSource *
						title <= cssLabel @ \.css
						Content <= CssSource $mol_textarea
							value?val <=> css_source?val \
							hint <= css_source_hint @ \view.css source
	Menu $mol_page
		title <= title_default \asdgas
		minimal_width 400
		body /
			<= Menu_links $mol_list
				rows <= menu_rows /
	-
	Details!id $mol_page
		minimal_width 600
		title <= question_title!id \
		event_top?val <=> event_front_up?val
		tools /
			<= Details_permalink!id $mol_link
				uri <= question_permalink!id \
				sub /
					<= Details_permalink_icon!id $mol_icon_external
			<= Details_close!id $mol_link
				sub /
					<= Details_close_icon!id $mol_icon_cross
				arg *
					question null
		body /
			<= Details_descr!id $mol_text
				text <= question_descr!id \
			<= Answers!id $mol_list
				rows <= answers!id /
	-
	Answer!id $mol_text
		text <= question_answer!id \
	-
	Question_link!index $mol_link
		minimal_height 64
		arg <= question_arg_by_index!index *
		sub /
			<= Question_title!index $mol_view
				sub /
					<= question_title_by_index!index \
			<= Question_tags!index $mol_view
				sub <= question_tags_by_index!index /
	-
	Tag!id $mol_view
		sub /
			<= tag_name!id \ 
